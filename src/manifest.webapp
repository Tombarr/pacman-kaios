  },
  "categories": [
    "games"
  ],
  "installs_allowed_from": [
    "*"
  ],
  "permissions": {
    "audio-channel-content": { },
    "service-worker": { }
  },
  "locales": {
    "en-US": {
      "name": "Pac-Man",
