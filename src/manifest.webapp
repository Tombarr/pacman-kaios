{
  "name": "Pak-Man",
  "version": "1.0.0",
  "launch_path": "/index.html",
  "description": "The classic arcade game Pac-Man for KaiOS",
  "type": "privileged",
  "icons": {
    "128": "/assets/images/launcher-icon-2x.png"
  },
  "fullscreen": "true",
